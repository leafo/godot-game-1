; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="test1"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[display]

window/size/width=240
window/size/height=136
window/size/test_width=480
window/size/test_height=272
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[layer_names]

2d_physics/layer_1="Wall"
2d_physics/layer_2="Player"
2d_physics/layer_3="Player Bullet"
2d_physics/Test=false

[rendering]

quality/intended_usage/framebuffer_allocation=1
quality/2d/use_pixel_snap=true
environment/default_clear_color=Color( 0.129412, 0.0509804, 0.0784314, 1 )
environment/default_environment="res://default_env.tres"
